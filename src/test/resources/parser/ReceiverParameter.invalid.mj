class MyJavaClass {

    public int counter;

    public void dec(MyJavaClass this) {
        this.counter = this.counter - 1;
    }

    public static void main(String[] args) {
        MyJavaClass javaClass = new MyJavaClass();
        javaClass.counter = 77;
        System.out.write(javaClass.counter);
        javaClass.dec();
        javaClass.dec();
        javaClass.dec();
        System.out.write(javaClass.counter);
    }
}
